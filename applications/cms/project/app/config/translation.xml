<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" xmlns="http://agavi.org/agavi/config/parts/translation/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
    <ae:configuration>

        <available_locales default_locale="de_DE">
            <available_locale identifier="de_DE">
                <ae:parameter name="description">Deutsch</ae:parameter>
            </available_locale>
        </available_locales>

        <translators default_domain="modules.labels">
            <translator domain="modules">
                <message_translator class="AgaviSimpleTranslator">
                     <ae:parameter name="labels">
                        <ae:parameter name="de">
                            <ae:parameter name="brand-name">ars-data</ae:parameter>
                            <ae:parameter name="brand-title">ars-data</ae:parameter>
                            <ae:parameter name="place">Orte</ae:parameter>
                            <ae:parameter name="Place">Orte</ae:parameter>
                            <ae:parameter name="layer">Layer</ae:parameter>
                            <ae:parameter name="Layer">Layer</ae:parameter>
                            <ae:parameter name="category">Kategorien</ae:parameter>
                            <ae:parameter name="Category">Kategorien</ae:parameter>
                            <ae:parameter name="user">CMS Nutzer</ae:parameter>
                            <ae:parameter name="User">CMS Nutzer</ae:parameter>
                            <ae:parameter name="tag">Schlagworte</ae:parameter>
                            <ae:parameter name="Tag">Schlagworte</ae:parameter>
                            <ae:parameter name="link">Links</ae:parameter>
                            <ae:parameter name="Link">Links</ae:parameter>
                            <ae:parameter name="brand-title">ars-data</ae:parameter>
                            <!-- german project module-name translations go here -->
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="brand-name">ars-data</ae:parameter>
                            <ae:parameter name="brand-title">ars-data</ae:parameter>
                            <!-- english project module-name translations go here -->
                        </ae:parameter>
                    </ae:parameter>
                </message_translator>
        </translator>
        <!-- example for translating custom roles.
        <translator domain="user">
            <message_translator class="AgaviSimpleTranslator">
                 <ae:parameter name="roles">
                    <ae:parameter name="de">
                        <ae:parameter name="third-party-editor">Nutzer mit eingeschr. Rechten (Drittanbieter)</ae:parameter>
                    </ae:parameter>
                </ae:parameter>
            </message_translator>
        </translator> -->
    </translators>

    </ae:configuration>
</ae:configurations>
